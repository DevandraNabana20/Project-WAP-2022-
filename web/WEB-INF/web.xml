<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Koneksi</servlet-name>
        <servlet-class>config.Koneksi1</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>C_Login</servlet-name>
        <servlet-class>controller.C_Login1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>C_Registrasi1</servlet-name>
        <servlet-class>controller.C_Registrasi1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>controller.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Koneksi</servlet-name>
        <url-pattern>/Koneksi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>C_Login</servlet-name>
        <url-pattern>/C_Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>C_Registrasi1</servlet-name>
        <url-pattern>/C_Registrasi1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
